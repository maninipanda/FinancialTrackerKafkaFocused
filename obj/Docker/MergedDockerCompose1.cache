name: dockercompose17375424529934277390
services:
  financialtracker.api:
    build:
      context: C:\MRepos\FinancialTrackerKafkaFocused
      dockerfile: FinancialTrackerKafkaFocused/Dockerfile
    container_name: financialtracker.api
    depends_on:
      kafka:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: financialtrackerapi
    networks:
      app-net: null
    ports:
      - mode: ingress
        target: 7238
        published: "7238"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: volume
        source: https
        target: /https
        volume: {}
      - type: bind
        source: C:\Users\akshy\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\akshy\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\akshy\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\akshy\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  kafka:
    container_name: kafka
    depends_on:
      zookeeper:
        condition: service_started
        required: true
    environment:
      KAFKA_ADVERTISED_LISTENERS: PLAINTEXT://kafka:9092
      KAFKA_BROKER_ID: "1"
      KAFKA_LISTENERS: PLAINTEXT://0.0.0.0:9092
      KAFKA_OFFSETS_TOPIC_REPLICATION_FACTOR: "1"
      KAFKA_TRANSACTION_STATE_LOG_MIN_ISR: "1"
      KAFKA_TRANSACTION_STATE_LOG_REPLICATION_FACTOR: "1"
      KAFKA_ZOOKEEPER_CONNECT: zookeeper:2181
    image: confluentinc/cp-kafka:7.4.0
    networks:
      app-net: null
    ports:
      - mode: ingress
        target: 9092
        published: "9092"
        protocol: tcp
  worker:
    build:
      context: C:\MRepos\FinancialTrackerKafkaFocused
      dockerfile: NotificationService/Dockerfile
    container_name: notificationservice-worker
    depends_on:
      kafka:
        condition: service_started
        required: true
    environment:
      KAFKA_BOOTSTRAP_SERVERS: kafka:9092
    image: notificationservice
    networks:
      app-net: null
  zookeeper:
    environment:
      ZOOKEEPER_CLIENT_PORT: "2181"
      ZOOKEEPER_TICK_TIME: "2000"
    image: confluentinc/cp-zookeeper:7.4.0
    networks:
      app-net: null
    ports:
      - mode: ingress
        target: 2181
        published: "2181"
        protocol: tcp
networks:
  app-net:
    name: dockercompose17375424529934277390_app-net
    driver: bridge
volumes:
  https:
    name: dockercompose17375424529934277390_https